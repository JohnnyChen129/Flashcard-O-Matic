 new AbortController();\r\n\r\n    async function loadDecks() {\r\n      try {\r\n        const loadedDecks = await listDecks();\r\n        setDecks([...loadedDecks]);\r\n      } catch (err) {\r\n        throw err;\r\n      }\r\n    }\r\n    loadDecks();\r\n    return abortCon.abort();\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"d-flex flex-column\">\r\n      <div className=\"mb-2\">\r\n        <Link className=\"btn btn-secondary\" to=\"/decks/new\">\r\n          <i className=\"fa-solid fa-plus\"></i> Create Deck\r\n        </Link>\r\n      </div>\r\n\r\n      <div>\r\n        <DeckList decks={decks} />\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","import React from \"react\";\r\n\r\nexport default function Card({ card, count, index, flipped, flip, next }) {\r\n  const nextButton = (\r\n    <button type=\"button\" className=\"btn btn-primary\" onClick={next}>\r\n      Next\r\n    </button>\r\n  );\r\n  return (\r\n    <>\r\n      <div className=\"card\">\r\n        <div className=\"card-body\">\r\n          <h4 className=\"card-title\">\r\n            Card {index} of {count}\r\n          </h4>\r\n          <p className=\"card-text\">{!flipped ? card.front : card.back}</p>\r\n          <button\r\n            type=\"button\"\r\n            className=\"btn btn-secondary mr-2\"\r\n            onClick={flip}\r\n          >\r\n            Flip\r\n          </button>\r\n          {flipped && nextButton}\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n","import React from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nexport default function NeedMoreCards({ id, cards }) {\r\n  return (\r\n    <div className=\"d-flex flex-column\">\r\n      <h3>Not enough cards.</h3>\r\n      <p>\r\n        You need at least 3 cards to study. There are {cards} cards in this\r\n        deck.\r\n      </p>\r\n      <div>\r\n        <Link className=\"btn btn-primary\" to={`/decks/${id}/cards/new`}>\r\n          <i className=\"fa-solid fa-plus\"></i> A